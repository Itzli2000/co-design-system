{
  "name": "@codigo-obsidiana/design-tokens",
  "version": "0.1.0",
  "description": "Design tokens for the CÃ³digo Obsidiana Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/codigo-obsidiana/toolkit.git",
    "directory": "packages/libs/design-tokens"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "design-tokens",
    "style-dictionary",
    "design-system",
    "css-variables",
    "tokens"
  ],
  "type": "module",
  "main": "./dist/obsidiana/index.js",
  "module": "./dist/obsidiana/index.js",
  "types": "./dist/obsidiana/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/obsidiana/index.d.ts",
      "import": "./dist/obsidiana/index.js",
      "require": "./dist/obsidiana/index.js",
      "default": "./dist/obsidiana/index.js"
    },
    "./css": "./dist/obsidiana/css/_variables.css",
    "./*/css": "./dist/*/css/_variables.css",
    "./*/css/": "./dist/*/css/index.css",
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js",
      "default": "./dist/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "style-dictionary": "^5.0.0"
  }
}
